{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"declaration": true,
		"moduleResolution": "Node",
		"strict": false,
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": ["src"]
}
